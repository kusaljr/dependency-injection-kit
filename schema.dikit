

model barcode {
  id int @primary_key @default(autoincrement())
  code string @unique @required
  is_active boolean @default(true)

  metadata json[] {
    description: string
    non_vegetarian: boolean
    type : {
      nonce: string
      hash: string
    }
    ingredients: string[]
    calories?: number    
  }

  created_at datetime @default(now()) 
  updated_at datetime 
}
